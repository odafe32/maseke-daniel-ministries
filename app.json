{
  "expo": {
    "name": "maseke-daniel-ministries",
    "slug": "maseke-daniel-ministries",
    "version": "1.0.0",
    "icon": "./src/assets/icon.svg",
    "userInterfaceStyle": "light",
    "scheme": "maseke-daniel-ministries",
    "newArchEnabled": false,
    "extra": {
      "eas": {
       "projectId": "36a3c62a-c2e6-4941-836d-f7bb175e9353"
      }
    },
    "plugins": ["expo-env"],
    "splash": {
      "image": "./src/assets/icon/icon.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maseke.mobile",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon/icon.svg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.maseke.mobile",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
            "versionCode": 2
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
